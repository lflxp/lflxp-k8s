#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/opt/gopath/src/github.com/lflxp/lflxp-k8s/shadcn-admin/node_modules/.pnpm/knip@5.41.1_@types+node@22.10.1_typescript@5.7.2/node_modules/knip/bin/node_modules:/opt/gopath/src/github.com/lflxp/lflxp-k8s/shadcn-admin/node_modules/.pnpm/knip@5.41.1_@types+node@22.10.1_typescript@5.7.2/node_modules/knip/node_modules:/opt/gopath/src/github.com/lflxp/lflxp-k8s/shadcn-admin/node_modules/.pnpm/knip@5.41.1_@types+node@22.10.1_typescript@5.7.2/node_modules:/opt/gopath/src/github.com/lflxp/lflxp-k8s/shadcn-admin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/opt/gopath/src/github.com/lflxp/lflxp-k8s/shadcn-admin/node_modules/.pnpm/knip@5.41.1_@types+node@22.10.1_typescript@5.7.2/node_modules/knip/bin/node_modules:/opt/gopath/src/github.com/lflxp/lflxp-k8s/shadcn-admin/node_modules/.pnpm/knip@5.41.1_@types+node@22.10.1_typescript@5.7.2/node_modules/knip/node_modules:/opt/gopath/src/github.com/lflxp/lflxp-k8s/shadcn-admin/node_modules/.pnpm/knip@5.41.1_@types+node@22.10.1_typescript@5.7.2/node_modules:/opt/gopath/src/github.com/lflxp/lflxp-k8s/shadcn-admin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/bun" ]; then
  exec "$basedir/bun"  "$basedir/../knip/bin/knip-bun.js" "$@"
else
  exec bun  "$basedir/../knip/bin/knip-bun.js" "$@"
fi
